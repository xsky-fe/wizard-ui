  "homepage": "https://xsky-fe.github.io/wizard-ui/",
  "files": [
    "CHANGELOG.md",
    "lib",
    "esm",
    "dist"
  ],
  "keywords": [
    "reactjs",
    "ui",
    "ui-component",
    "gatsby-site"
