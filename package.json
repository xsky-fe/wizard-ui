    "@types/react-dom": "16.8.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "classnames": "^2.2.6",
    "conventional-changelog-cli": "^2.0.21",
    "core-js": "^3.1.4",
