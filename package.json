    "react-syntax-highlighter": "^7.0.4",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.3.0",
    "ts-jest": "^24.0.2",
    "typescript": "3.4.5",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
